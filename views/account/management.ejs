<!-- content heading -->
<% if (title) { %>
<h1><%= title %></h1>
<% } else {
  res.redirect('/')
} %>

<!-- message -->
<% if (notice && notice.length > 0) { %>
    <div class="notice">
        <%= notice %>
    </div>
<% } %>

<!-- errors -->
<% if (errors && errors.length > 0) { %>
    <div class="errors">
      <% errors.forEach(function(error){ %>
        <p><%= error.msg %></p>
      <% }) %>
    </div>
<% } %>

<p>You're logged in.</p>